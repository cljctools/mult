{:deps         true
 :nrepl        {:port 8899
                :host "0.0.0.0"
                :init-ns dev.shadow}

 :http {:port 9630
        :host "0.0.0.0"}
 :builds
 {:app {:target :browser
        :output-dir "resources/public/js-out"
        :asset-path "/js-out"
        :module-loader true
        :modules
        {:main {:entries [fruits.banana]}}
        :compiler-options
        {:infer-externs    :auto
         :externs ["datascript/externs.js"]
         :closure-warnings {:global-this :off}
         :closure-defines  {"foo.bar_some_val_"         true}}

        :devtools
        {:repl-init-ns fruits.banana
         :repl-pprint true
         :autobuild false
         :http-root  "resources/public"
         :devtools-url "http://localhost:9630"
         :http-port  9500
       ;   :preloads   []
         }}}}
